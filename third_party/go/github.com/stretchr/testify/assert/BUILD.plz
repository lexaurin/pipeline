    import_path = "github.com/stretchr/testify/assert",
    visibility = ["PUBLIC"],
    deps = [
        "//third_party/go/github.com/davecgh/go-spew/spew",
        "//third_party/go/github.com/pmezard/go-difflib/difflib",
        "//third_party/go/gopkg.in/yaml.v3",
    ],
