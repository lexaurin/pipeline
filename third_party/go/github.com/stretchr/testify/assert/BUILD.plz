subinclude("///pleasegomod//:build_defs")

fileexport(
    name = "assert",
    srcs = [
        "assert/assertion_compare.go",
        "assert/assertion_format.go",
        "assert/assertion_forward.go",
        "assert/assertions.go",
        "assert/doc.go",
        "assert/errors.go",
