subinclude("///pleasegomod//:build_defs")

fileexport(
    name = "binding",
    srcs = [
        "binding/binding.go",
        "binding/default_validator.go",
        "binding/form.go",
        "binding/form_mapping.go",
        "binding/header.go",
        "binding/json.go",
        "binding/msgpack.go",
        "binding/multipart_form_mapping.go",
        "binding/protobuf.go",
        "binding/query.go",
        "binding/uri.go",
        "binding/xml.go",
        "binding/yaml.go",
