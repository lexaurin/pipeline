    cover = False,
    import_path = "github.com/gin-gonic/gin/binding",
    visibility = ["PUBLIC"],
    deps = [
        "//third_party/go/github.com/gin-gonic/gin/internal/json",
        "//third_party/go/github.com/golang/protobuf/proto",
        "//third_party/go/github.com/ugorji/go/codec",
        "//third_party/go/gopkg.in/go-playground/validator.v9",
        "//third_party/go/gopkg.in/yaml.v2",
    ],
