subinclude("///pleasegomod//:build_defs")

fileexport(
    name = "docker",
    srcs = [
        "remotes/docker/auth.go",
        "remotes/docker/authorizer.go",
        "remotes/docker/converter.go",
        "remotes/docker/fetcher.go",
        "remotes/docker/handler.go",
