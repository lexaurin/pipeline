        "//third_party/go/github.com/containerd/containerd/remotes/docker/schema1",
        "//third_party/go/github.com/containerd/containerd/version",
        "//third_party/go/github.com/docker/distribution/registry/api/errcode",
        "//third_party/go/github.com/opencontainers/go-digest",
        "//third_party/go/github.com/opencontainers/image-spec/specs-go/v1",
        "//third_party/go/github.com/pkg/errors",
        "//third_party/go/github.com/sirupsen/logrus",
        "//third_party/go/golang.org/x/net/context/ctxhttp",
    ],
