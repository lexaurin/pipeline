subinclude("///pleasegomod//:build_defs")

fileexport(
    name = "cmp",
    srcs = [
        "cmp/compare.go",
        "cmp/export_panic.go",
        "cmp/export_unsafe.go",
        "cmp/options.go",
        "cmp/path.go",
        "cmp/report.go",
        "cmp/report_compare.go",
        "cmp/report_reflect.go",
        "cmp/report_slices.go",
        "cmp/report_text.go",
        "cmp/report_value.go",
    ],
    tag = "go_source",
    deps = ["//third_party/go/github.com/google/go-cmp:_go-cmp#download"],
)

go_library(
    name = "cmp",
    srcs = [":_cmp#go_source"],
    cover = False,
    import_path = "github.com/google/go-cmp/cmp",
    visibility = ["PUBLIC"],
    deps = [
        "//third_party/go/github.com/google/go-cmp/cmp/internal/diff",
        "//third_party/go/github.com/google/go-cmp/cmp/internal/flags",
        "//third_party/go/github.com/google/go-cmp/cmp/internal/function",
        "//third_party/go/github.com/google/go-cmp/cmp/internal/value",
    ],
)
