subinclude("///pleasegomod//:build_defs")

fileexport(
    name = "grpc",
    srcs = [
        "transport/grpc/client.go",
        "transport/grpc/doc.go",
        "transport/grpc/encode_decode.go",
        "transport/grpc/request_response_funcs.go",
        "transport/grpc/server.go",
    ],
    tag = "go_source",
    deps = ["//third_party/go/github.com/go-kit/kit:_kit#download"],
)

go_library(
    name = "grpc",
    srcs = [":_grpc#go_source"],
    cover = False,
    import_path = "github.com/go-kit/kit/transport/grpc",
    visibility = ["PUBLIC"],
    deps = [
        "//third_party/go/github.com/go-kit/kit/endpoint",
        "//third_party/go/github.com/go-kit/kit/log",
        "//third_party/go/github.com/go-kit/kit/transport",
        "//third_party/go/google.golang.org/grpc",
        "//third_party/go/google.golang.org/grpc/metadata",
    ],
)
