subinclude("///pleasegomod//:build_defs")

fileexport(
    name = "sdk",
    srcs = [
        "sdk/api_timeout.go",
        "sdk/client.go",
        "sdk/config.go",
        "sdk/logger.go",
    ],
    tag = "go_source",
    deps = ["//third_party/go/github.com/aliyun/alibaba-cloud-sdk-go:_alibaba-cloud-sdk-go#download"],
