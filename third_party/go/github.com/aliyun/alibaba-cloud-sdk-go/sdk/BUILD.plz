        "//third_party/go/github.com/aliyun/alibaba-cloud-sdk-go/sdk/endpoints",
        "//third_party/go/github.com/aliyun/alibaba-cloud-sdk-go/sdk/errors",
        "//third_party/go/github.com/aliyun/alibaba-cloud-sdk-go/sdk/requests",
        "//third_party/go/github.com/aliyun/alibaba-cloud-sdk-go/sdk/responses",
        "//third_party/go/github.com/aliyun/alibaba-cloud-sdk-go/sdk/utils",
    ],
)
