subinclude("///pleasegomod//:build_defs")

fileexport(
    name = "client",
    srcs = [
        "aws/client/client.go",
        "aws/client/default_retryer.go",
        "aws/client/logger.go",
        "aws/client/no_op_retryer.go",
    ],
    tag = "go_source",
    deps = ["//third_party/go/github.com/aws/aws-sdk-go:_aws-sdk-go#download"],
