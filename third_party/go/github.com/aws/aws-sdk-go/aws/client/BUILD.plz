go_library(
    name = "client",
    srcs = [":_client#go_source"],
    cover = False,
    import_path = "github.com/aws/aws-sdk-go/aws/client",
    visibility = ["PUBLIC"],
    deps = [
        "//third_party/go/github.com/aws/aws-sdk-go/aws",
        "//third_party/go/github.com/aws/aws-sdk-go/aws/client/metadata",
        "//third_party/go/github.com/aws/aws-sdk-go/aws/request",
        "//third_party/go/github.com/aws/aws-sdk-go/internal/sdkrand",
    ],
)
