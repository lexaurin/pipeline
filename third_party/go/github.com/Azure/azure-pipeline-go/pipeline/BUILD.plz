    srcs = [
        "pipeline/core.go",
        "pipeline/defaultlog.go",
        "pipeline/defaultlog_syslog.go",
        "pipeline/defaultlog_windows.go",
        "pipeline/doc.go",
        "pipeline/error.go",
        "pipeline/progress.go",
        "pipeline/request.go",
        "pipeline/response.go",
        "pipeline/version.go",
