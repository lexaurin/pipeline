go_library(
    name = "pipeline",
    srcs = [":_pipeline#go_source"],
    cover = False,
    import_path = "github.com/Azure/azure-pipeline-go/pipeline",
    visibility = ["PUBLIC"],
    deps = ["//third_party/go/github.com/mattn/go-ieproxy"],
