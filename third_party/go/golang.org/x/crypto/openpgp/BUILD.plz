subinclude("///pleasegomod//:build_defs")

fileexport(
    name = "openpgp",
    srcs = [
        "openpgp/canonical_text.go",
        "openpgp/keys.go",
        "openpgp/read.go",
        "openpgp/write.go",
    ],
    tag = "go_source",
    deps = ["//third_party/go/golang.org/x/crypto:_crypto#download"],
)

go_library(
    name = "openpgp",
    srcs = [":_openpgp#go_source"],
    cover = False,
    import_path = "golang.org/x/crypto/openpgp",
    visibility = ["PUBLIC"],
    deps = [
        "//third_party/go/golang.org/x/crypto/openpgp/armor",
        "//third_party/go/golang.org/x/crypto/openpgp/errors",
        "//third_party/go/golang.org/x/crypto/openpgp/packet",
        "//third_party/go/golang.org/x/crypto/openpgp/s2k",
    ],
)
