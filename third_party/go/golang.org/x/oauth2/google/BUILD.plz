subinclude("///pleasegomod//:build_defs")

fileexport(
    name = "google",
    srcs = [
        "google/appengine.go",
        "google/appengine_gen1.go",
        "google/appengine_gen2_flex.go",
        "google/default.go",
        "google/doc.go",
        "google/google.go",
        "google/jwt.go",
        "google/sdk.go",
    ],
    tag = "go_source",
    deps = ["//third_party/go/golang.org/x/oauth2:_oauth2#download"],
)

go_library(
    name = "google",
    srcs = [":_google#go_source"],
    cover = False,
    import_path = "golang.org/x/oauth2/google",
    visibility = ["PUBLIC"],
    deps = [
        "//third_party/go/cloud.google.com/go/compute/metadata",
        "//third_party/go/golang.org/x/oauth2",
        "//third_party/go/golang.org/x/oauth2/internal",
        "//third_party/go/golang.org/x/oauth2/jws",
        "//third_party/go/golang.org/x/oauth2/jwt",
    ],
)
