    import_path = "google.golang.org/protobuf/runtime/protoiface",
    visibility = ["PUBLIC"],
    deps = [
        "//third_party/go/google.golang.org/protobuf/internal/pragma",
        "//third_party/go/google.golang.org/protobuf/reflect/protoreflect",
    ],
