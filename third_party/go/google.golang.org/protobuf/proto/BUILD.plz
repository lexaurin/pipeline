subinclude("///pleasegomod//:build_defs")

fileexport(
    name = "proto",
    srcs = [
        "proto/checkinit.go",
        "proto/decode.go",
        "proto/decode_gen.go",
        "proto/doc.go",
        "proto/encode.go",
        "proto/encode_gen.go",
        "proto/equal.go",
        "proto/extension.go",
        "proto/merge.go",
        "proto/messageset.go",
        "proto/proto.go",
        "proto/proto_methods.go",
        "proto/proto_reflect.go",
        "proto/reset.go",
        "proto/size.go",
        "proto/size_gen.go",
        "proto/wrappers.go",
    ],
    tag = "go_source",
    deps = ["//third_party/go/google.golang.org/protobuf:_protobuf#download"],
)

go_library(
    name = "proto",
    srcs = [":_proto#go_source"],
    cover = False,
    import_path = "google.golang.org/protobuf/proto",
    visibility = ["PUBLIC"],
    deps = [
        "//third_party/go/google.golang.org/protobuf/encoding/protowire",
        "//third_party/go/google.golang.org/protobuf/internal/encoding/messageset",
        "//third_party/go/google.golang.org/protobuf/internal/errors",
        "//third_party/go/google.golang.org/protobuf/internal/fieldsort",
        "//third_party/go/google.golang.org/protobuf/internal/flags",
        "//third_party/go/google.golang.org/protobuf/internal/mapsort",
        "//third_party/go/google.golang.org/protobuf/internal/pragma",
        "//third_party/go/google.golang.org/protobuf/internal/strs",
        "//third_party/go/google.golang.org/protobuf/reflect/protoreflect",
        "//third_party/go/google.golang.org/protobuf/reflect/protoregistry",
        "//third_party/go/google.golang.org/protobuf/runtime/protoiface",
    ],
)
